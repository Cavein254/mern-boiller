{
  "name": "react-boiler-plate",
  "version": "1.0.0",
  "description": "react boiler plate",
  "main": "index.js",
  "engines": {
    "node": "10.19.0",
    "yarn": "1.22.4",
    "npm": "6.14.4"
  },
  "scripts": {
    "start": "node server/index.js",
    "backend": "nodemon server/index.js",
    "frontend": "npm start --prefix client",
    "dev": "concurrently \"yarn run backend\" \"npm start --prefix client\"",
    "heroku-postbuild": "yarn install && cd client && yarn install && npm run build"
  },
