    "bcrypt": "^4.0.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "global": "^4.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "mpesa-node": "^0.1.3",
    "multer": "^1.4.2",
    "ngrok": "^3.2.7",
    "node": "^14.0.0",
    "parse-json": "^5.0.0",
    "request": "^2.88.2",
    "requests": "^0.3.0",
    "saslprep": "^1.0.3",
    "supports-color": "^7.1.0"
  },
